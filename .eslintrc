{
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "never",
        "objects": "always",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }
    ],
    "semi": ["error", "always"],
    "quotes": ["error", "single"]
  },
  "env": {
    "es6": true
  }
}
